{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "quackback-web-dev",
  "account_id": "67cf7d5c8b797bfd5cbd417b233cc0d0",
  "main": "../../apps/web/dist/server/index.js",
  "compatibility_date": "2025-12-01",
  "compatibility_flags": ["nodejs_compat"],
  // Disable esbuild's keepNames to prevent __name helper from leaking into
  // seroval's serialized stream code. See: https://opennext.js.org/cloudflare/howtos/keep_names
  "keep_names": false,
  "assets": {
    "directory": "../../apps/web/dist/client"
  },
  "routes": [
    {
      "pattern": "main.dev.quackback.io/*",
      "zone_name": "quackback.io"
    },
    {
      "pattern": "storefeeder.dev.quackback.io/*",
      "zone_name": "quackback.io"
    }
  ],
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      "service": "quackback-web-dev"
    }
  ],
  "vars": {
    "ROOT_URL": "https://main.dev.quackback.io",
    "BETTER_AUTH_URL": "https://main.dev.quackback.io",
    "EMAIL_FROM": "Quackback Dev <dev@quackback.io>",
    "NODE_ENV": "development",
    "EDITION": "cloud",
    "CLOUD_TENANT_BASE_DOMAIN": "dev.quackback.io",
    "CLOUD_NEON_DEFAULT_REGION": "aws-eu-west-2",
    // OpenAI via Cloudflare AI Gateway
    "OPENAI_BASE_URL": "https://gateway.ai.cloudflare.com/v1/67cf7d5c8b797bfd5cbd417b233cc0d0/quackback-dev/openai"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "invocation_logs": true
    }
  }
}
